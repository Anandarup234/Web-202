{
  "name": "biryani",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass ./src/main.scss ./src/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass dev",
    "compile:sass": "node-sass ./src/main.scss ./dist/css/style.comp.css",
    "concat:css": "concat -o ./dist/css/style.concat.css ./dist/css/style.comp.css ./src/css/icon.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" -c \"{grid:true}\" <./dist/css/style.concat.css> ./dist/css/style.prefix.css",
    "compress:css": "node-sass ./dist/css/style.prefix.css ./src/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "dev": "webpack --mode development --env.dist=false -w",
    "dev:dist": "webpack --mode development",
    "build": "webpack --config webpack.prod.js",
    "startwebpack": "webpack-dev-server --open --config webpack.dev.js",
    "start:server": "webpack-dev-server --open --config webpack.prod.js",
    "startwp": "npm-run-all --parallel startwebpack dev"
  },
  "author": "Jonas",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "concat": "^1.0.3",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "path": "^0.12.7",
    "postcss": "^7.0.1",
    "postcss-loader": "^2.1.6",
    "precss": "^3.1.2",
    "sass-loader": "^7.0.3"
  }
}
